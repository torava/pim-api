{
  "name": "bookkeepr",
  "version": "0.0.17",
  "description": "bookkeepr",
  "main": "src/server.js",
  "scripts": {
    "test": "jest",
    "build": "./node_modules/.bin/webpack --mode production",
    "build:dev": "./node_modules/.bin/webpack --mode development",
    "start": "node src/server-compiled.js",
    "knex": "./node_modules/.bin/babel-node ./node_modules/.bin/knex",
    "client": "./node_modules/.bin/webpack serve --mode development --env development",
    "server": "./node_modules/.bin/nodemon --watch ./src --ignore src/components --max-old-space-size=8192 src/server.js --exec ./node_modules/.bin/babel-node",
    "dev": "export NODE_ENV=development && ./node_modules/.bin/concurrently 'npm:server' './node_modules/.bin/wait-on http://localhost:42809/api/brand && npm run client'"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/src/__mocks__/setupJestMock.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "author": "Teemu Orava",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "apicache": "^1.6.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "canvas": "^2.7.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "csv-parse": "^4.15.1",
    "exceljs": "^4.2.1",
    "express": "^4.16.4",
    "jimp": "^0.12.1",
    "jsdom": "^13.2.0",
    "knex": "^0.21.17",
    "lodash": "^4.17.21",
    "match-sorter": "^2.3.0",
    "moment": "^2.22.2",
    "mysql": "^2.18.1",
    "natural": "^0.6.3",
    "objection": "^2.2.15",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.6.347",
    "pg": "^8.5.1",
    "quagga": "^0.12.1",
    "react": "^16.13.0",
    "react-asterisk-table": "^2.0.12",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.2",
    "serverless-http": "^2.7.0",
    "stream": "0.0.2",
    "string-similarity-js": "^2.1.2",
    "swagger-ui-express": "^4.1.6",
    "tesseract.js": "^2.1.4",
    "victory": "^35.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.1",
    "@types/pdfjs-dist": "^2.1.7",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.0",
    "css-loader": "^5.2.5",
    "eslint": "^7.19.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.6",
    "sass-loader": "^11.0.1",
    "serverless-webpack": "^5.4.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^4.0.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
