{
  "name": "product-api",
  "version": "0.0.36",
  "description": "product-api",
  "scripts": {
    "test": "jest --env=jsdom",
    "build": "./node_modules/.bin/webpack --mode production",
    "build:dev": "./node_modules/.bin/webpack --mode development",
    "start": "node src/server-compiled.js",
    "knex": "./node_modules/.bin/babel-node ./node_modules/.bin/knex --env production",
    "dev": "export NODE_ENV=development && ./node_modules/.bin/nodemon --watch ./src --max-old-space-size=8192 src/server.js --exec ./node_modules/.bin/babel-node"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/src/__mocks__/setupJestMock.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "author": "Teemu Orava",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "apicache": "^1.6.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "canvas": "^2.8.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "crypto": "^1.0.1",
    "csv-parse": "^4.15.1",
    "exceljs": "^4.3.0",
    "express": "^4.16.4",
    "jimp": "^0.12.1",
    "jsdom": "^17.0.0",
    "knex": "^0.95.10",
    "lodash": "^4.17.21",
    "match-sorter": "^2.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.10.0",
    "natural": "^5.1.0",
    "objection": "^2.2.15",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.6.347",
    "pg": "^8.5.1",
    "quagga": "^0.12.1",
    "request": "^2.88.2",
    "stream": "0.0.2",
    "string-similarity-js": "^2.1.2",
    "swagger-ui-express": "^4.1.6",
    "tesseract.js": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-regenerator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@types/pdfjs-dist": "^2.1.7",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.3.2",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "wait-on": "^4.0.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  }
}
