  "description": "bookkeepr",
  "main": "src/server.js",
  "scripts": {
    "test": "jest",
    "build": "./node_modules/.bin/webpack --mode production",
    "build:dev": "./node_modules/.bin/webpack -d -w",
    "start": "npm run build && node src/server-compiled.js",
    "client": "./node_modules/.bin/webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "./node_modules/.bin/nodemon --watch ./src --ignore src/components --max-old-space-size=8192 src/server.js --exec ./node_modules/.bin/babel-node",
    "dev": "export NODE_ENV=development && ./node_modules/.bin/concurrently 'npm:server' './node_modules/.bin/wait-on http://localhost:42809/api/manufacturer && npm run client'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
