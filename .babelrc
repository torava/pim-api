{
  "ignore": ["src/static"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    "@babel/transform-runtime"
  ],
  "presets": [  "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "exclude": ["babel-plugin-transform-classes"]
      }
    ]
  ]
}